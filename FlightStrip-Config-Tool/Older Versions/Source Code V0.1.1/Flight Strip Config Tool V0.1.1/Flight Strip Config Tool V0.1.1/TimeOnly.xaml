<Window x:Class="Flight_Strip_Config_Tool_WPF_V0._1.TimeOnly"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        Title="Effekt Parameter" Height="178" Width="335" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="effektLabel" Content="Effekt :" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="savebtn" Content="Speichern" HorizontalAlignment="Left" Margin="10,106,0,0" VerticalAlignment="Top" Width="110" Click="savebtn_Click"/>
        <Button x:Name="cancelbtn" Content="Abbrechen" HorizontalAlignment="Left" Margin="202,106,0,0" VerticalAlignment="Top" Width="110" Click="cancelbtn_Click"/>
        <TextBox x:Name="effectText" HorizontalAlignment="Left" Height="23" Margin="61,14,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="251"/>
        <Label Content="Zeit bis zum Wechsel :" HorizontalAlignment="Left" Margin="10,59,0,0" VerticalAlignment="Top"/>
        <xctk:IntegerUpDown x:Name="time" HorizontalAlignment="Left" Margin="143,63,0,0" VerticalAlignment="Top" Width="78"/>
        <Label Content="Millisekunden" HorizontalAlignment="Left" Margin="226,59,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
